{
  "name": "my-awesome-portfolio",
  "version": "1.0.3",
  "homepage": "https://thomasgroch.com.br/",
  "description": "Just playing around",
  "author": "Thomas Letsch Groch <thomas.groch@gmail.com>",
  "license": "MIT",
  "scripts": {
    "serve": "yarn build && http-server src/.vuepress/dist/ -c-1 -o -a localhost",
    "dev": "node_modules/.bin/vuepress dev src --host localhost",
    "build": "node_modules/.bin/vuepress build src",
    "setup-language": "cd src && ln -s ../content/pt/* .",
    "setup-gpg": "git config --global gpg.program $(which gpg)",
    "lint": "eslint --ext .js,.vue --ignore-path .gitignore .",
    "precommit": "yarn lint"
  },
  "devDependencies": {
    "eslint": "^4.19.1",
    "gitmoji-cli": "^1.9.2"
  },
  "dependencies": {
    "vuepress": "next",
    "dotenv": "^6.1.0",
    "markdown-it-katex": "^2.0.3",
    "vuepress-theme-yuu": "^1.0.0"
  },
  "repository": {
    "type": "git",
    "url": "https://gitlab.com/thomas.groch/vuepress-theme-thgroch.git"
  },
  "keywords": [
    "blog",
    "developer",
    "vuepress",
    "markdown",
    "static",
    "vuejs"
  ]
}
